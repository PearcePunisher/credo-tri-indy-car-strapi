{
    "kind": "collectionType",
    "collectionName": "up_users",
    "info": {
        "name": "user",
        "description": "",
        "singularName": "user",
        "pluralName": "users",
        "displayName": "User"
    },
    "options": {
        "draftAndPublish": false
    },
    "attributes": {
        "username": {
            "type": "string",
            "minLength": 3,
            "unique": true,
            "configurable": false,
            "required": true
        },
        "email": {
            "type": "email",
            "minLength": 6,
            "configurable": false,
            "required": true
        },
        "provider": {
            "type": "string",
            "configurable": false
        },
        "password": {
            "type": "password",
            "minLength": 6,
            "configurable": false,
            "private": true,
            "searchable": false
        },
        "resetPasswordToken": {
            "type": "string",
            "configurable": false,
            "private": true,
            "searchable": false
        },
        "confirmationToken": {
            "type": "string",
            "configurable": false,
            "private": true,
            "searchable": false
        },
        "confirmed": {
            "type": "boolean",
            "default": false,
            "configurable": false
        },
        "blocked": {
            "type": "boolean",
            "default": false,
            "configurable": false
        },
        "role": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.role",
            "inversedBy": "users",
            "configurable": false
        },
        "user_image": {
            "type": "media",
            "multiple": false,
            "required": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "user_first_name": {
            "type": "string"
        },
        "user_last_name": {
            "type": "string"
        },
        "user_date_of_birth": {
            "type": "date"
        },
        "user_sponsor_inviter": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::sponsor.sponsor"
        },
        "user_children": {
            "displayName": "user_children",
            "type": "component",
            "repeatable": true,
            "component": "shared.user-children"
        },
        "signed_waiver": {
            "type": "boolean"
        },
        "signed_waiver_link": {
            "type": "string"
        },
        "users_guests": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::users-guest.users-guest",
            "mappedBy": "users_guest_parent_user"
        }
    }
}
